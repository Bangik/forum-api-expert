{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "class-methods-use-this":"off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off"
    }
}
